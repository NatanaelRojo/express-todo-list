{
  "name": "express-todo-list",
  "version": "1.0.0",
  "description": "This is a backend to todo list",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "migrations:generate": "sequelize-cli migration:generate --name ",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:revert": "sequelize-cli db:migrate:undo",
    "migrations:delete": "sequelize-cli db:migrate:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NatanaelRojo/express-todo-list.git"
  },
  "keywords": [
    "node",
    "todolist",
    "backend",
    "api"
  ],
  "author": "Natanael Rojo <rojonatanael99@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NatanaelRojo/express-todo-list/issues"
  },
  "homepage": "https://github.com/NatanaelRojo/express-todo-list#readme",
  "dependencies": {
    "@hapi/boom": "^10.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "joi": "^17.7.0",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.26.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.20",
    "sequelize-cli": "^6.5.2"
  }
}